r=\"永辉超市优惠商品\"\n        />\n    </div>    \n</div>\n<div class=\"ad-content\">\n        <img id=\"ad-img\" src=\"@/assets/images/02.jpg\" alt=\" \">\n</div>\n<div class=\"menu-content\">\n    <ul class=\"menu-list\">\n        <li v-for=\"data in menuData\" class=\"main-menu\" :key=\"data.name\">\n            <img :src=\"data.src\" alt=\" \">\n            <span class=\"menu-text\">{{ data.name }}</span>\n        </li>\n    </ul>\n</div>\n<div class=\"block-content\"></div>\n<div class=\"data-content\">\n    <div class=\"data-side-text\">\n        <span class=\"left-text\">附近店铺</span>\n        <span class=\"right-text\">查看更多</span>\n    </div>\n    <div class=\"main-data\">\n        <ul style=\"list-style: none\">\n            <li class=\"shop\" v-for=\"info in shopInfo\" :key=\"info.id\">\n                <img class=\"shop-icon\" :src=\"info.imgUrl\" alt=\"\"/>\n                <div class=\"shop-info\">\n                    <p class=\"shop-name\">\n                        {{info.name}}\n                    </p>\n                    <br/>\n                    <p class=\"base-info\">\n                        <span class=\"sale-count\">\n                            月售:{{info.sales}}\n                        </span>\n                        <span class=\"lowest-deliver-price\">\n                            起送:{{info.expressLimit}}\n                        </span>\n                        <span class=\"normal-deliver-price\">\n                            基础运费: {{info.expressPrice}}\n                        </span>\n                    </p>\n                    <br>\n                    <p class=\"discount-info\">\n                        {{info.slogan}}\n                    </p>\n                </div>\n            </li>\n        </ul>\n    </div>\n</div>\n\n<div class=\"function-menu-content\">\n    <ul style=\"list-style: none;\">\n        <li class=\"function-menu\"><img class=\"function-menu-icon\" src=\"mark/assets/首页.png\" alt=\"\"><span class=\"function-menu-text\">首页</span></li>\n        <li class=\"function-menu\"><img class=\"function-menu-icon\" src=\"mark/assets/cart_empty.png\" alt=\"\"><span class=\"function-menu-text\">购物车</span></li>\n        <li class=\"function-menu\"><img class=\"function-menu-icon\" src=\"mark/assets/order.png\" alt=\"\"><span class=\"function-menu-text\">订单</span></li>\n        <li class=\"function-menu\"><img class=\"function-menu-icon\" src=\"mark/assets/My.png\" alt=\"\"><span class=\"function-menu-text\">我的</span></li>\n    </ul>\n</div>\n<dir></dir>\n\n</template>\n\n<script>\nexport default {\n    \n    name: \"Home\",\n    data(){\n        return {\n            menuData: [\n                {\n                    name: \"超市便利\",\n                    src: \"/mark/assets/super.png\"\n                },\n                {\n                    name: \"菜市场\",\n                    src: \"/mark/assets/market.png\"\n                },\n                {\n                    name: \"水果店\",\n                    src: \"/mark/assets/fruits.png\"\n                },\n                {\n                    name: \"鲜花绿植\",\n                    src: \"/mark/assets/flower.png\"\n                },\n                {\n                    name: \"医药健康\",\n                    src: \"/mark/assets/drug.png\"\n                },\n                {\n                    name: \"家居时尚\",\n                    src: \"/mark/assets/home.png\"\n                },\n                {\n                    name: \"烘焙蛋糕\",\n                    src: \"/mark/assets/cake.png\"\n                },\n                {\n                    name: \"签到\",\n                    src: \"/mark/assets/sign.png\"\n                },\n                {\n                    name: \"大牌免运\",\n                    src: \"/mark/assets/prestige.png\"\n                },\n                {\n                    name: \"红包套餐\",\n                    src: \"/mark/assets/red.png\"\n                },\n                \n            ],\n            shopInfo: [],\n        }\n    },\n    methods: {\n        getShopData(){\n            let SHOP_DATA_URL = \"https://labfile.oss.aliyuncs.com/courses/9617/hotList.json\"\n             this.axios.get(SHOP_DATA_URL)\n            .then((res) => {\n                this.shopInfo = res.data.data;\n                console.log(this.shopInfo)\n            });\n            \n        },\n\n    },\n    mounted(){\n        this.getShopData()\n    }\n}\n</script>\n<style>\n* {\n    padding: 0;\n    margin: 0;\n}\n.top-content {\n    width: 750px;\n    height: 204px;\n    background-color: #3B91F8;\n}\n.location-content {\n    margin-left: 36px;\n    margin-top: 33px;\n    /* border: 2px solid red; */\n    width: 300px;\n    height: 42px;\n    display: inline-block;\n}\n#location-text {\n    position: relative;\n    bottom: 10px;\n    font-family: PingFangSC-Medium;\n    font-size: 31px;\n    color: #FFFFFF;\n    letter-spacing: 1.15px;\n    text-align: justify;\n}\n#search-box {\n    background: #FFFFFF;\n    border-radius: 34.5px;\n}\n.notice-content {\n    height: 42px;\n    width: 42px;\n    /* border: 2px solid red; */\n    display: inline-block;\n    margin-left: 337px;\n}\n.search-content {\n    margin-top: 37px;\n    height: 69px;\n    width: 680px;\n    /* border: 2px solid red; */\n    text-align: center;\n    margin-left: 36px;\n}\n\n#search-box {\n    background: #FFFFFF;\n    border-radius: 34.5px;\n    display: inline-block;\n    height: 69px;\n    width: 680px;\n\n}\n.ad-content {\n    width: 750px;\n    height: 192px;\n}\n#ad-img {\n    width: 750px;\n    height: 192px;\n}\n.menu-content {\n    background-image: linear-gradient(\n        180deg, \n        #5294EB 0%, \n        #FFFFFF 100%);\n    width: 750px;\n    height: 334px;\n}\n.menu-list {\n    list-style: none;\n}\n.main-menu {\n    width: 18%;\n    height: 120px;\n    display: inline-block;\n    margin: 5px;\n    margin-top: 30px;\n    text-align: center;\n}\n.menu-text {\n    font-family: PingFangSC-Regular;\n    font-size: 24px;\n    color: #20364B;\n    letter-spacing: 0;\n    text-align: center;\n    display: block;\n}\n.block-content {\n    width: 750px;\n    height: 20px;\n    background-color: #F1F1F1;\n}\n.left-text {\n    float: left;\n    font-family: PingFangSC-Regular;\n    font-size: 29px;\n    color: #20364B;\n    letter-spacing: -0.7px;\n}\n.right-text {\n    float: right;\n    font-family: PingFangSC-Regular;\n    font-size: 29px;\n    color: #20364B;\n    letter-spacing: -0.7px;\n}\n\n.data-side-text {\n    overflow: auto;\n    width: 680px;\n    margin-left: 35.5px;\n    margin-top: 25px;\n    margin-bottom: 25px;\n    /* border: 2px solid red; */\n\n}\n.shop {\n    width: 750px;\n    height: 198px;\n    border-top: 2px solid rgba(241,241,241,1);\n    border-bottom: 2px solid rgba(241,241,241,1);\n}\n.shop-icon {\n    margin-top: 25px;\n    margin-left: 36px;\n    width: 112px;\n    height: 112px;\n    display: inline-block;\n    border-radius: 50%;\n}\n.shop-info {\n    height: 150px;\n    width: 425px;\n    margin-left: 32px;\n    margin-bottom: 25px;\n    margin-top: 25px;\n    display: inline-block;\n}\n.shop-name {\n    font-family: PingFangSC-Regular;\n    font-size: 32px;\n    color: #20364B;\n    letter-spacing: -0.77px;\n}\n.base-info {\n    font-family: PingFangSC-Regular;\n    font-size: 26px;\n    color: #20364B;\n    letter-spacing: -0.1px;\n}\n.discount-info {\n    font-family: PingFangSC-Regular;\n    font-size: 26px;\n    color: #E27475;\n    letter-spacing: 0;\n}\n\n.function-menu-content {\n    height: 100px;\n    width: 750px;\n}\n\n.function-menu-text {\n    font-family: PingFangSC-Regular;\n    font-size: 23px;\n    color: #353535;\n    letter-spacing: 0;\n    text-align: center;\n    display: block;\n}\n.function-menu-icon {\n    width: 45px;\n    height: 45px;\n}\n\n.function-menu {\n    width: auto;\n    height: auto;\n    float: left;\n    margin-left: 15% ;\n    margin-top: 18px;\n    margin-bottom: 10px;\n    \n}\n\n</style>\n"]}]}